<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="3" parallel ="tests">

		<listeners>
			<listener class-name="extendReport.ExtentTestNGIReporterListener"/>
		</listeners>

	<test name="ChromeTest">
		<parameter name="BrowserName" value="Chrome"/>
		    <classes>
		  	      <class name="TestNG.TestNG_FLIPKART"/>
			      <class name="TestNG.TestNG_AMAZON"/>
			      <class name="TestNG.TestNG_TESTAUTOMATION"/>
			      <class name="TestNG.TestNG_YOUTUBE"/>
